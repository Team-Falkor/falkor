{
  "name": "falkor",
  "description": "Dive into Falkor, where you'll discover a vast selection of games coupled with community-driven enhancements. Experience seamless and secure gaming within a single, universal hub.",
  "version": "0.6.2-beta",
  "main": "dist-electron/main/index.js",
  "license": "BSD-3-Clause",
  "private": true,
  "author": {
    "name": "team-falkor",
    "url": "https://github.com/team-falkor"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/team-falkor/falkor.git"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "tsc && vite build",
    "build": "tsc && vite build && electron-builder",
    "build:linux": "electron-builder --linux --publish always",
    "build:win": "electron-builder --win --publish always",
    "build:mac": "electron-builder --mac --publish always",
    "preview": "vite preview",
    "pretest": "vite build --mode=test",
    "test": "vitest run",
    "postinstall": "electron-builder install-app-deps",
    "rebuild": "electron-rebuild -f -w better-sqlite3",
    "check": "biome check .",
    "check:unsafe": "biome check --write --unsafe .",
    "check:write": "biome check --write .",
    "db:generate": "drizzle-kit generate"
  },
  "dependencies": {
    "@expo/sudo-prompt": "^9.3.2",
    "@hookform/resolvers": "^5.1.1",
    "@janwirth/electron-trpc-link": "^11.1.2-bugfix-1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "^5.81.2",
    "@tanstack/react-router": "^1.121.34",
    "@tanstack/react-router-devtools": "^1.121.34",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/zod-adapter": "^1.121.34",
    "@team-falkor/shared-types": "^1.0.6",
    "@trpc/client": "^11.4.2",
    "@trpc/react-query": "^11.4.2",
    "@trpc/server": "^11.4.2",
    "@zip.js/zip.js": "^2.7.62",
    "audio-decode": "^2.2.3",
    "auto-launch": "^5.0.6",
    "better-sqlite3": "^12.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.44.2",
    "electron-updater": "^6.3.9",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "fuse.js": "^7.1.0",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "jsonc-parser": "^3.3.1",
    "lucide-react": "^0.523.0",
    "next-themes": "^0.4.6",
    "node-unrar-js": "^2.0.2",
    "open": "^10.1.2",
    "radix-ui": "^1.4.2",
    "react-day-picker": "^9.7.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.58.1",
    "react-i18next": "^15.5.3",
    "react-icons": "^5.5.0",
    "sonner": "^2.0.5",
    "speaker": "^0.5.5",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.4",
    "webtorrent": "^2.6.8",
    "zod": "^3.25.67",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.5",
    "@playwright/test": "^1.48.2",
    "@tailwindcss/vite": "^4.1.5",
    "@tanstack/router-plugin": "^1.121.37",
    "@types/auto-launch": "^5.0.5",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.0.4",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@types/webtorrent": "^0.110.0",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "drizzle-kit": "^0.31.2",
    "electron": "^33.2.0",
    "electron-builder": "^24.13.3",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "postcss": "^8.4.49",
    "postcss-import": "^16.1.0",
    "react": "^19.1.0",
    "tailwindcss": "^4.1.8",
    "typescript": "^5.4.2",
    "vite": "^6.3.5",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vitest": "^2.1.5"
  }
}
